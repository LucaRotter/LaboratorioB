<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane onMouseClicked="#onSelection" onMouseEntered="#OnMoveEnter" onMouseExited="#OnMoveExit" prefHeight="120.0" prefWidth="125.0" styleClass="libropane" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.libreriaController">
   <children>
      <VBox alignment="CENTER" prefHeight="120.0" prefWidth="126.0">
         <children>
            <ImageView fitHeight="65.0" fitWidth="95.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
               <image>
                  <Image url="@bookshelf-icon-png-6.jpg" />
               </image>
            </ImageView>
            <Label fx:id="nameLibrary" prefHeight="18.0" prefWidth="125.0" text="library name" textAlignment="CENTER" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
