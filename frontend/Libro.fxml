<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane onMouseClicked="#onSelect" onMouseEntered="#OnMoveEnter" onMouseExited="#OnMoveExit" prefHeight="110.0" prefWidth="125.0" styleClass="libropane" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.libroController">
   <children>
      <VBox alignment="CENTER" prefHeight="120.0" prefWidth="125.0" spacing="5.0">
         <children>
            <ImageView fitHeight="65.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
               <image>
                  <Image url="@Paomedia-Small-N-Flat-Book.512.png" />
               </image>
            </ImageView>
            <Label fx:id="nameBook" prefHeight="18.0" prefWidth="130.0" text="name book" textAlignment="CENTER" VBox.vgrow="ALWAYS" />
            <ImageView fitHeight="24.0" fitWidth="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@0-star.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
   </children>
</AnchorPane>
